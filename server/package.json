{
  "name": "resume-analysis-app",
  "version": "1.0.0",
  "description": "A web application for analyzing resumes and providing feedback.",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "test": "jest"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "multer": "1.4.4-lts.1",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.11",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "resume",
    "analysis",
    "job",
    "application"
  ],
  "author": "Your Name",
  "license": "MIT"
  ,
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  }
}